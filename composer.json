{
  "name": "joshuaanderton/notion",
  "description": "Nifty Notion API helper",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.1",
      "illuminate/support": "^8.67|^9.0",
  },
  "require-dev": {
      "phpunit/phpunit": "^9.3"
  },
  "autoload": {
      "psr-4": {
          "Ja\\Notion\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Ja\\Notion\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Ja\\Notion\\Providers\\ServiceProvider"
          ]
      }
  }
}
